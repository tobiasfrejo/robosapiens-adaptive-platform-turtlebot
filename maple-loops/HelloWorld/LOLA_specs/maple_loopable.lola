in stage : Str
out m : Bool
out a : Bool
out p : Bool
out l : Bool
out e : Bool
out maple : Bool
out stageout : Str
m = (stage == "m") && (e[-1, true] || a[-1, false]) 
a = (stage == "a") && m[-1, false] 
p = (stage == "p") && (a[-1, false] || l[-1, false]) 
l = (stage == "l") && p[-1, false]
e = (stage == "e") && l[-1, false]
maple = m || a || p || l || e
stageout = stage